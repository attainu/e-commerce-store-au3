{
  "name": "capstone_e_commerce",
  "version": "1.0.0",
  "description": "e-commerce app using react redux",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "server": "node app.js",
    "client": "npm start --prefix client",
    "install-all": "concurrently \"npm install\" \"npm run client-install\"",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "gaurav verma",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "recharts": "^2.0.0-beta.1",
    "sequelize": "^5.21.3"
  },
  "engines": {
    "node": "12.16.1"
  }
}
